{
  "name": "contiguity",
  "version": "0.0.9",
  "description": "Contiguity JavaScript/TypeScript SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "bun run build && vitest run",
    "prepublishOnly": "tsc",
    "pack": "bun pm pack",
    "publish:dry": "bun run build && bun publish --dry-run",
    "publish": "bun run build && npm publish"
  },
  "keywords": [
    "contiguity",
    "sms",
    "email",
    "otp",
    "imessage",
    "whatsapp"
  ],
  "license": "MIT",
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "@react-email/render": "^2.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependenciesMeta": {
    "@react-email/render": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@react-email/render": "^2.0.4"
  }
}